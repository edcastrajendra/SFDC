<apex:page controller="AutoCompleteController">
    <html>
        <apex:includeScript value="{!URLFOR($Resource.Assets_SU,'js/angular.min.js')}" />      
        <body>
            <div ng-app="myApp" ng-controller="namesCtrl as vm">
                
                <p>Type a letter in the input field:</p>
                
                <p><input type="text" ng-model="vm.test" /></p>
                <p ng-click="vm.getValue()">
                    click me
                </p>
                <ul>
                    <li ng-repeat="x in vm.names | filter:vm.test">
                        {{ x }}
                    </li>
                </ul>
                
            </div>
            
            <script>
            angular.module('myApp', []).controller('namesCtrl', function() {
                
                var vm = this;
                vm.getValue = function(){
                    console.log(vm.test);
                     AutoCompleteController.getAutoComplete(vm.test,'1', function(result, event){
                    console.log(result);
                     });
                    }
                
                
               
                vm.names = [
                    'Jani',
                    'Carl',
                    'Margareth',
                    'Hege',
                    'Joe',
                    'Gustav',
                    'Birgit',
                    'Mary',
                    'Kai'
                ];
            });
            </script>
            
            <p>The list will only consists of names matching the filter.</p>
        </body>
    </html>
</apex:page>