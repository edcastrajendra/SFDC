            <apex:form id="frm">
                
                <script>
                    
                    $(document).ready(function(){
                        $(".slds-button").removeClass("btn");
                    });
                
                    function init(){
                        try{
                            var searchTags = $.parseJSON('{!JSENCODE(searchCourses)}');
                            $(".searchTags").autocomplete({
                                source: searchTags
                                
                            });
                           
                            var availableTags = $.parseJSON('{!JSENCODE(usersStr)}');
                            $(".tags").tagEditor({
                                autocomplete: {
                                    delay: 0,
                                    position: {
                                        collision: 'flip'
                                    },
                                    source: availableTags
                                },
                                forceLowercase: false
                            });
                            
                            $('.left-div').height('auto');
                            $('.right-div').height('auto');
                            if($('.left-div').height() <= $('.right-div').height())
                                $('.left-div').height( $('.right-div').height());
                            else
                                $('.right-div').height( $('.left-div').height());
                        }catch(err ){}
                    }
                </script>
                <apex:actionFunction action="{!changeRole}" name="changeRole" reRender="frm" status="status" oncomplete="init();" />
                <apex:actionFunction action="{!changeGroup}" name="changeGroup" reRender="frm" status="status" oncomplete="init();" />
                <apex:actionFunction name="addRemoveRoles" action="{!addRemoveRoles}" reRender="frm" status="status">
                    <apex:param name="param1" assignTo="{!buttonType}" value="" />
                </apex:actionFunction>
               
                <apex:actionFunction name="addRemoveGroup" action="{!addRemoveGroup}" reRender="frm" status="status">
